<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace：名称空间 可以随便写一个名字 -->
<mapper namespace="com.wsw.dao.AccountDao">
    <!--id是唯一标识，resultType对应POJO类-->
    <select id="selectAll" resultType="com.wsw.domain.Account">
        select * from tb_account;
    </select>
</mapper>